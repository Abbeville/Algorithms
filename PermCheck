// you can write to stdout for debugging purposes, e.g.
// console.log('this is a debug message');

function solution(A) {
    // write your code in JavaScript (Node.js 8.9.4)
    for(let i = 1; i <= A.length; i++){
        let permutation = false;
        for(let j = 0; j < A.length; j++) {
            if(i == A[j]) {
                permutation = true;
                continue;
            }
        }

        if(!permutation){
            return 0
        }
    }

    return 1;
}
